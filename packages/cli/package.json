{
  "name": "create-vita",
  "description": "Scaffold a new Vite + TSX + Alpine.js project",
  "type": "module",
  "version": "1.0.8",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billybillydev/create-vita.git",
    "directory": "packages/cli"
  },
  "scripts": {
